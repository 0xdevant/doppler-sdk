  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "src"
  },
  "packageManager": "yarn@1.22.18",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc watch ./src/**",
