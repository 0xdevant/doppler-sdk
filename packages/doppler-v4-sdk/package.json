    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@delvtech/drift": "^0.0.1-beta.25",
    "@delvtech/drift-viem": "^0.1.0-beta.30",
    "@types/node": "^22.9.4",
    "@uniswap/sdk-core": "^5.9.0",
    "ethers": "^5.0.8",
